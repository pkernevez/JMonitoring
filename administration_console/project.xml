            </properties>
        </dependency>    
        
        <dependency>
            <groupId>jfreechart</groupId>
            <artifactId>jcommon</artifactId>
            <version>${depends.jfreechartcommon.version}</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>    
        
       

