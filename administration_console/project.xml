            </properties>
        </dependency>

       
        <dependency>
          <groupId>mysql</groupId>
          <artifactId>mysql-connector-java</artifactId>
          <version>${depends.mysql.version}</version>
          <url>http://www.mysql.com/</url>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>
        
        <dependency>
          <id>servletapi</id>
          <version>${depends.servletapi.version}</version>
          <url>http://www.sun.com/</url>
        </dependency>
        
        <dependency>
            <id>struts</id>
            <version>${depends.struts.version}</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>    
        
        <dependency>
            <id>struts-menu</id>
            <version>${depends.strutsmenu.version}</version>
