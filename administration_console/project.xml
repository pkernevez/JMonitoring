            </properties>
        </dependency>

        <dependency>
          <id>commons-digester</id>
          <version>${depends.commonsdigester.version}</version>
          <url>http://www.apache.org/</url>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
          <id>commons-beanutils</id>
          <version>${depends.commonsbeanutils.version}</version>
          <url>http://www.apache.org/</url>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
          <id>commons-fileupload</id>
          <version>${depends.commonsfileupload.version}</version>
          <url>http://www.apache.org/</url>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
