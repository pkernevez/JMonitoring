<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:org.jmonitoring.console.gwt.client">
	<ui:with
		type="org.jmonitoring.console.gwt.client.resources.ConsoleImageBundle"
		field="res"></ui:with>
	<ui:style src="../resources/main.css" />
		
	<g:DockLayoutPanel styleName="{style.main}" unit="PX">
		<g:north size="65">
			<g:HorizontalPanel ui:field="header" styleName="{style.header}">
				<g:Image resource="{res.jmonitoring}" />
			</g:HorizontalPanel>
		</g:north>
		<g:west size='150'>
			<p1:Menu ui:field="menu" styleName="{style.menu}" />
		</g:west>
		<g:center>
			<g:SimplePanel ui:field="content" styleName="{style.main-panel}"/>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 