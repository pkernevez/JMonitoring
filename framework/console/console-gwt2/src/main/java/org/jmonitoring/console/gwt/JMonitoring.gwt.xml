<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='JMonitoring'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <inherits name="com.google.gwt.logging.Logging"/>
  <inherits name="com.google.gwt.activity.Activity"/>
  <inherits name='it.pianetatecno.gwt.utility.GwtUtility'/>
  
  <set-property name="gwt.logging.logLevel" value="FINE"/>          # To change the default logLevel
  <set-property name="gwt.logging.enabled" value="TRUE"/>            # To disable logging
  <set-property name="gwt.logging.firebugHandler" value="ENABLED" />
  <set-property name="gwt.logging.consoleHandler" value="DISABLED" /> # To di√æsable a default Handler
  <set-property name="gwt.logging.popupHandler" value="DISABLED" />
  
  
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.jmonitoring.console.gwt.client.JMonitoring'/>


<stylesheet src="GwtUtility.css"/>

  <!-- Possible value : ie6,ie8,gecko1_8,safari,opera -->
  <!--set-property name="user.agent" value="ie6"/-->
<!-- <stylesheet src=""/> -->

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
