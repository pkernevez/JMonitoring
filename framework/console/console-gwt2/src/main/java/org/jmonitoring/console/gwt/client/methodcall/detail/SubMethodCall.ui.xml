<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../../resources/main-style.css" />
		
	<ui:with field="image"
		type="org.jmonitoring.console.gwt.client.resources.ConsoleImageBundle" />
	<g:HorizontalPanel>
		<g:Label ui:field="prevDuration"></g:Label>
		<g:Label ui:field="localDuration"></g:Label>
		<g:Label ui:field="text"></g:Label>
		<g:Image resource="{image.edit}" styleName="{style.clickable}" ui:field="edit"/> 
		<g:Image resource="{image.graphique}" styleName="{style.clickable}" ui:field="stats"/>
	</g:HorizontalPanel>
</ui:UiBinder> 