<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with
		type="org.jmonitoring.console.gwt.client.resources.ConsoleImageBundle"
		field="res"></ui:with>
	<ui:style src="../resources/main.css" />
	<g:VerticalPanel styleName="{style.searchmask}">
		<g:HTML><h1>Flow Search</h1></g:HTML> 
		<g:Grid>
			<g:row>
				<g:cell><div class="{style.label}">Thread</div></g:cell>
				<g:customCell><g:TextBox ui:field="thread"></g:TextBox></g:customCell>
				<g:cell><div class="{style.label}">Minimum Duration</div></g:cell>
				<g:customCell><g:TextBox ui:field="minDuration"></g:TextBox></g:customCell>
			</g:row>
			<g:row>
				<g:cell><div class="{style.label}">Group</div></g:cell>
				<g:customCell><g:TextBox ui:field="group"></g:TextBox></g:customCell>
				<g:cell><div class="{style.label}">Begin Date (dd/MM/yy)</div></g:cell>
				<g:customCell><g:TextBox ui:field="beginDate"></g:TextBox></g:customCell>
			</g:row>
			<g:row>
				<g:cell><div class="{style.label}">First Measure Class Name</div></g:cell>
				<g:customCell><g:TextBox ui:field="firstMeasureClassName"></g:TextBox></g:customCell>
				<g:cell><div class="{style.label}">First Measure Method Name</div></g:cell>
				<g:customCell><g:TextBox ui:field="firstMeasureMethodName"></g:TextBox></g:customCell>
			</g:row>
		</g:Grid>
		<g:Image resource="{res.ok}" ui:field="image"></g:Image>
	</g:VerticalPanel>
</ui:UiBinder> 