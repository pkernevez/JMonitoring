            <Item title="Nice Looking Tabbed Menu" toolTip="Shows Velocity generated Tabbed Menu" page="/velocity-nicetabs.jsp?Home" /> 
            <Item title="EL (Expression Language) enabled Menu [New!]" toolTip="Shows very simple Velocity EL Menu" page="/velocity-el.jsp" /> 
            <Item title="Dynamic, Databased-driven Menu [New!]" toolTip="Shows very simple menu built from a database table" page="/dynamicMenu.jsp" /> 
