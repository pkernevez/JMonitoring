<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <bean name="storeManager" class="org.jmonitoring.agent.store.StoreManagerBean" scope="prototype">
        <property name="serverName" value="TestMachine"/>
        <property name="storeWriter" ref="storeWriter"/>
    </bean>
        
    <bean name="storeWriter" class="org.jmonitoring.agent.store.impl.MemoryWriter" scope="prototype"/>
    
</beans>
